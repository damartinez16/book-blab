<%- include('../partials/header') %>
<table id="list"></table>
  <thead>
    <tr>
      <th>Title</th> 
      <th>Author</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    <% books.forEach(function(b) { %>
      <tr>
        <td><%= b.title %></td>
        <td><%= b.author %></td>
        <td><%= b.genre %></td>
        <td><a href="/books/<%= b._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
<%- include('../partials/footer') %>
